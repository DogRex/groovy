<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="org.codehaus.groovy"
   name="org.codehaus.groovy"
   version="1.0.0"
   provider-name="codehaus"
   class="groovy.GroovyRuntimePlugin">

   <runtime>
      <library name="asm-1.4.1.jar">
         <export name="*"/>
      </library>
      <library name="asm-attrs-1.4.1.jar">
         <export name="*"/>
      </library>
      <library name="asm-util-1.4.1.jar">
         <export name="*"/>
      </library>
      <library name="groovy-1.0-beta-4-snapshot.jar">
         <export name="*"/>
      </library>
      <library name="Groovy.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.core.runtime.compatibility"/>
   </requires>


</plugin>
