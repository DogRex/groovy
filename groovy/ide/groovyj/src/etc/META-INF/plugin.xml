<?xml version="1.0" encoding="UTF-8"?>

<idea-plugin url="@PROJECT.HOMEPAGE@" version="2">

    <name>@PROJECT.TITLE@</name>
    <description>@PROJECT.DESCRIPTION@</description>
    <version>@PROJECT.VERSION@</version>
    <vendor logo="/icons/groovy_16x16.png" url="@PROJECT.VENDOR.HOMEPAGE@" email="@PROJECT.VENDOR.EMAIL@">@PROJECT.VENDOR@</vendor>
    <change-notes>@PROJECT.CHANGE-NOTES@</change-notes>
    <idea-version since-build="@PROJECT.IDEA.SINCE-BUILD@" until-build="@PROJECT.IDEA.UNTIL-BUILD@"/>

    <extensions xmlns="com.intellij">
        <component area="IDEA_PROJECT">
            <implementation>org.codehaus.groovy.intellij.GroovyJProjectComponent</implementation>
        </component>
    </extensions>

    <extensions xmlns="com.intellij">
        <component>
            <implementation>org.codehaus.groovy.intellij.EditorAPIFactory</implementation>
        </component>
        <component>
            <implementation>org.codehaus.groovy.intellij.configuration.GroovyConfigurationType</implementation>
        </component>
        <component>
            <implementation>org.codehaus.groovy.intellij.GroovySupportLoader</implementation>
        </component>
    </extensions>

</idea-plugin>
